#!/usr/bin/perl -w

=head1 NAME

example-list - Geo:Ellipsiods example

=cut

use strict;
use lib q{../lib};
use lib q{./lib};
use Geo::Ellipsiods;

my $object=Geo::Ellipsoids->new()
               || die("Error: Cannot create object.");

my @list=$object->list();
while (sort @list) {
  print $_->[0], "\n";
}
